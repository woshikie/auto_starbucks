{
	"name": "Auto Starbucks",
	"version": "1.0",
	"description": "Extension to auto refresh starbucks",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
	},
	"permissions": [
		"alarms",
		"webRequest",
		"webRequestBlocking",
		"*://*.d-synergy.com/*"
	]
}
